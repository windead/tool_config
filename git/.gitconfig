[merge]
	tool = vimdiff
[alias]
	co = checkout
	br = branch
	ci = commit
	st = status
    l = log --pretty=oneline -n 20 --graph --abbrev-commit 
    ll = log --color --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit -- 
	d = difftool
[push]
	default = simple
[credential]
	helper = store
[diff]
	tool = vimdiff
[difftool]
	prompt = false
